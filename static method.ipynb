    "class Student:\n",
    "# Static method\n",
    "    @staticmethod\n",
    "    def get_student_details(name, roll):\n",
    "        print(\"Name:\", name)\n",
    "        print(\"Roll No.:\", roll)\n",
    "\n",
    "# Driver code \n",
    "name = \"Nandhana\"\n",
    "roll = \"35\"\n",
    "\n",
    "# call the static method\n",
    "Student.get_student_details(name, roll)"
